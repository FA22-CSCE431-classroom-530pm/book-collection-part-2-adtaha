<p id="notice"><%= notice %></p>

<h1>Member Orgs</h1>

<table>
  <thead>
    <tr>
      <th>Memberorgid</th>
      <th>Memberid</th>
      <th>Organizationid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @member_orgs.each do |member_org| %>
      <tr>
        <td><%= member_org.memberOrgID %></td>
        <td><%= member_org.memberID %></td>
        <td><%= member_org.organizationID %></td>
        <td><%= link_to 'Show', member_org %></td>
        <td><%= link_to 'Edit', edit_member_org_path(member_org) %></td>
        <td><%= link_to 'Destroy', member_org, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member Org', new_member_org_path %>
