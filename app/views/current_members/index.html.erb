<p id="notice"><%= notice %></p>

<h1>Current Members</h1>

<table>
  <thead>
    <tr>
      <th>Memberid</th>
      <th>Name</th>
      <th>Email</th>
      <th>Isadmin</th>
      <th>Isalumni</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @current_members.each do |current_member| %>
      <tr>
        <td><%= current_member.memberID %></td>
        <td><%= current_member.name %></td>
        <td><%= current_member.email %></td>
        <td><%= current_member.isAdmin %></td>
        <td><%= current_member.isAlumni %></td>
        <td><%= link_to 'Show', current_member %></td>
        <td><%= link_to 'Edit', edit_current_member_path(current_member) %></td>
        <td><%= link_to 'Destroy', current_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Current Member', new_current_member_path %>
